zone "rpz.example.com" {
	type master;
	file "/etc/bind/db.rpz";
	allow-query { localhost; };
};

zone "_acme-le.example.com" {
	type forward;
	forward only;
	forwarders {
		203.0.113.22; // _acme-le.example.com
	};
};

